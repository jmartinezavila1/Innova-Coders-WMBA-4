<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - WMBA_4</title>
    @* favicon *@
    <link rel="icon" href="~/WMBA-icon-36x36.png" type="image/x-icon" />
    <link rel="shortcut icon" href="~/WMBA-icon-36x36.png" type="image/x-icon" />
     @* bootstrap *@
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.min.css">
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/style.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/layoutStyle.css" asp-append-version="true" />
    <link href="~/css/scorekeeperstyle.css" rel="stylesheet" asp-append-version="true" />
    @* google icons *@
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@24,400,0,0" />
    @* fontawesome *@
    <script src="https://use.fontawesome.com/releases/v6.3.0/js/all.js" crossorigin="anonymous"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    @* below script added by Kim for Home index *@
    <script src="https://cdn.jsdelivr.net/npm/masonry-layout@4.2.2/dist/masonry.pkgd.min.js" integrity="sha384-GNFwBvfVxBkLMJpYMOABq3c+d3KnQxudP/mGPkzpZSTYykLBNsZEnG2D9G/X/+7D" crossorigin="anonymous" async></script>
    @*  Scorekeeperview-- Cristina Added *@
    @RenderSection("Styles", required: false)
    @* Message confirmation run or out *@
    <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
</head>
<body class="sb-nav-fixed">
    <nav class="sb-topnav navbar bg-light-grey n-in-flex">
        <div>
            <!-- Sidebar Toggle-->
            <button class="btn btn-link btn-sm order-lg-1 ms-5 me-lg-0" id="sidebarToggle" href="#!"><i class="fas fa-bars ft-kelly fs-5"></i></button>
        </div>
        <!-- Navbar Brand-->
        <a class="navbar-brand d-flex justify-content-center" asp-area="" asp-controller="Home" asp-action="Index"><img src="~/images/WMBA.png" alt="WBMA logo" class="logo" /></a>
        <!-- Setting-->
        <div class="me-5">
            <div class="flex-shrink-0 dropdown">
                
                <a class="nav-link dropdown-toggle" id="navbarDropdown" href="#" role="button" data-bs-toggle="dropdown" aria-expanded="false" title="Settings">
                   @*  <i class="fa-solid fa-gear ft-kelly"></i> *@
                    <svg class="svg-inline--fa fa-user fa-fw ft-kelly" aria-hidden="true" focusable="false" data-prefix="fas" data-icon="user" role="img" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 448 512" data-fa-i2svg=""><path fill="currentColor" d="M224 256A128 128 0 1 0 224 0a128 128 0 1 0 0 256zm-45.7 48C79.8 304 0 383.8 0 482.3C0 498.7 13.3 512 29.7 512H418.3c16.4 0 29.7-13.3 29.7-29.7C448 383.8 368.2 304 269.7 304H178.3z"></path></svg>
                </a>
                <ul class="dropdown-menu dropdown-menu-lg-end mt-3">
                    @await Html.PartialAsync("_LoginPartial")
                </ul>
            </div>
        </div>
    </nav>
    <div id="layoutSidenav">
        <div id="layoutSidenav_nav">
            <nav class="sb-sidenav accordion bg-sidenav" id="sidenavAccordion">
                <partial name="_Sidebar" />
            </nav>
        </div>
        <div id="layoutSidenav_content">
            <main>
                <div class="container-fluid px-6 py-6">
                    @RenderBody()
                </div>
            </main>
            <footer class="bg-light mt-auto border-top">
                <div class="container-fluid px-6 py-4">
                    <partial name="_Footer" />
                </div>
            </footer>
        </div>
    </div>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/js/sidenav.js" asp-append-version="true"></script>
    <script src="~/js/checkbox.js" asp-append-version="true"></script>
    <script src="~/js/scorekeeper.js" asp-append-version="true"></script>
    <script>
        $(document).ready(function () {
            $("#ImportView").click(function () {
                window.location.href = '@Url.Action("GoToImportPlayers", "Team")';
            });
        });
    </script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
